<%# add navigation links to this file %>
<% if user_signed_in? %>

        <li>
          <%=
          link_to current_user.email, edit_user_registration_path
          %>
        </li>
        <li>
          <%=
          link_to 'Log Out', logout_path, method: :get
          %>
        </li>

        <% else %>

        <li>
          <%=
           link_to 'Sign In', login_path, class: "button"
          %>
        </li>

        <% end %>